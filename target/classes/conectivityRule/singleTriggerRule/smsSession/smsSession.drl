package conectivityRule.singleTriggerRule.smsSession 
import gcontrol.rule.model.SMSModel;
import gcontrol.rule.model.Trigger;
import gcontrol.rule.model.Validation;
import java.time.LocalDate;

rule "Valid SMS Session"
  dialect "mvel"
  when
 	$trigger:Trigger()
	$sms: SMSModel( smsTypeCount > $trigger.ruleOccurrenceCount )
  then
	
	modify($sms) {
		   setValidation( Validation.PASSED )
		  
		}
	
end

rule "INValid SMS Session"
  dialect "mvel"
  when
 	$trigger:Trigger()
	$sms: SMSModel( smsTypeCount < $trigger.ruleOccurrenceCount )
  then
	
	modify($sms) {
		   setValidation( Validation.FAILED )
		  
		}
	
end

